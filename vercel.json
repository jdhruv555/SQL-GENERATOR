{
    "version": 2,
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python",
            "config": { 
                "runtime": "python3.9",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ],
    "nodejs": {
        "version": "18.x"
    }
}
